page{background: #fff}

.my_Info{ text-align: center;  background: #fff;position: relative; height: 300rpx; padding-bottom: 20rpx;}
.my_InfoBg{ position: absolute; width: 100%; left: 0; top: 0; height: 300rpx;}
.my_InfoMain{ position: relative;padding: 10rpx 0 30rpx;}
