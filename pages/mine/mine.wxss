.my_pro{ display: block; height: 120rpx; width: 120rpx; border-radius: 50%; position: absolute; left: 0rpx; top: 0rpx; right: 0; bottom: 0; margin: auto; box-shadow: 0 0 0 4rpx rgba(255,255,255,0.4);}
.my_sex{ width:40rpx; height: 40rpx; position: absolute; right: 0; bottom: 0;}
.my_sexImg{  width:40rpx; height: 40rpx; border-radius: 50%;}
.my_name{ display: inline-block; color: #fff;  height: 60rpx; line-height: 60rpx;font-size: 34rpx; font-weight: bold; }
.my_listItem{ font-size:32rpx; color:#333; height: 130rpx; line-height: 130rpx; margin: 0 20rpx; border-top:1px solid #ebebeb ; margin-top: -1px;}
.my_listItemAct{ color: #ff6600; background: #EDEDED;}
.my_listItem_sub{ text-align: center; color: #999; font-size: 24rpx;}
.my_list{ margin-bottom: 15rpx; background: #fff; overflow: hidden;}